  </head>

  <body id="<%= body_id %>" class="<%= body_class %>">
    <ol class="breadcrumb" style="margin-bottom:0">
      <li><a href="/">Movies UK</a></li>
      <%= yield(:breadcrumbs) %>
    </ol>

    <div class="container-fluid">

      <header>
        <h1><%= yield(:page_header) %></h1>
      </header>

      <%= yield %>

      <hr />

      <div class="row">
        <div class="col-sm-12">
          <small>Coming Soon</small>
        </div>
      </div>
    </div>

    <%= javascript_include_tag 'application' %>
    <script>
      <%- action_method_for_js = "init_#{action_for_js}".camelize(:lower) -%>

      atthemovies.pages.init();
      if(!!atthemovies.pages.<%= controller_for_js -%>) {
        if(!!atthemovies.pages.<%= controller_for_js -%>.init) {
          atthemovies.pages.<%= controller_for_js -%>.init();
        }
        if(!!atthemovies.pages.<%= controller_for_js -%>.<%= action_method_for_js -%>) {
          atthemovies.pages.<%= controller_for_js -%>.<%= action_method_for_js -%>();
        }
      }
    </script>
  </body>
</html>
